<!DOCTYPE html>
<html>
<head>

    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Geincos</title>
    <link href="../../Content/css/bootstrap.min.css" rel="stylesheet" />
    <link href="../../Content/css/font-awesome.min.css" rel="stylesheet" />
    <link href="../../Content/css/styles.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-3.2.1.min.js"></script>
    <script src="../../Scripts/MediaLoot/bootstrap.min.js"></script>
    <script src="~/Scripts/moment.min.js"></script>
    <script src="~/Scripts/sweetalert.min.js"></script>
    <script src="~/Scripts/alertify.min.js"></script>
    <script src="~/Scripts/Pager.js"></script>
    <style>
        /* width */
        ::-webkit-scrollbar {
            width: 5px;
        }

        /* Track */
        ::-webkit-scrollbar-track {
            background: #f1f1f1;
        }

        /* Handle */
        ::-webkit-scrollbar-thumb {
            background: #888;
        }

            /* Handle on hover */
            ::-webkit-scrollbar-thumb:hover {
                background: #555;
            }
    </style>
    <script>
       
    </script>
</head>
<body>
    @using (Html.BeginForm("Sesion_Close", "Usuario", FormMethod.Post, new { id = "frmlogout" }))
            {
        @Html.AntiForgeryToken()
    }
    <nav class="navbar navbar-custom navbar-fixed-top" role="navigation">
        <div class="container-fluid">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#sidebar-collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="#"><span>Geincos</span></a>

            </div>
        </div>
    </nav>
    <div id="sidebar-collapse" class="col-sm-3 col-lg-2 sidebar">
        <div class="profile-sidebar">
            <div class="profile-userpic col-md-12 text-center">
                <img src="../Content/Image/businessman.png" class="img-responsive" alt="">
            </div>
            <div class="profile-usertitle">
                <div class="profile-usertitle-name">Test</div>
                <div class="profile-usertitle-status"><span class="indicator label-success"></span>Juan Carlos De la Cruz Chinga</div>
            </div>
            <div class="clear"></div>
        </div>
        <div class="divider"></div>
        <ul class="nav menu">
            <li id="Wm"><a href="@Url.Action("Index", "Alumno")"><em class="fa fa-user">&nbsp;</em> Alumnos</a></li>
            <li id="Em"><a href="@Url.Action("Index", "Curso")"><em class="fa fa-bar-chart">&nbsp;</em> Cursos</a></li>
            <li class="parent ">
                <a data-toggle="collapse" href="#sub-item-1">
                    <i class="fa fa-star">&nbsp;</i> Asignaturas <span data-toggle="collapse" href="#sub-item-1" class="icon pull-right"><em class="fa fa-plus"></em></span>
                </a>
                <ul class="children collapse" id="sub-item-1">

                    <li>
                        <a class="" href="@Url.Action("Inventario", "Almacen")">
                            <span class="fa fa-file-o">&nbsp;</span> Notas
                        </a>
                    </li>
                    <li>
                        <a class="" href="@Url.Action("Index", "Producto")">
                            <span class="fa fa-bar-chart">&nbsp;</span> Reporte
                        </a>
                    </li>
                </ul>
            </li>

            <li><a href="javascript:document.getElementById('frmlogout').submit()"><em class="fa fa-power-off">&nbsp;</em> Cerrar Sesion</a></li>
        </ul>
    </div>


    <!--/.sidebar-->

    <div class="col-sm-9 col-sm-offset-3 col-lg-10 col-lg-offset-2 main">
        <div class="row">
            <ol class="breadcrumb">
                <li>
                    <a href="#">
                        <em class="fa fa-home"></em>
                    </a>
                </li>
                <li class="active">Sistema de prueba</li>
            </ol>
        </div>
        @RenderBody()
    </div>
</body>
</html>
